			            Anobis_RLS_MV[city] = "cities"
			        )
			    )
			
			VAR SelectedCity = SELECTEDVALUE(City_Table[city])
			
			RETURN
			    IF(
			        HasAllAccess > 0 ||
			        SelectedCity IN UserCities,
			        1,
			        0
			    )
			
