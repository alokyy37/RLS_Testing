	measure Tooltip_Food_Last7Days = ```
			
			VAR FormatKMB = 
			    // reusable formatter
			    VAR num = 0
			    RETURN
			        SWITCH(
			            TRUE(),
			            ISBLANK(num), "0.00",
			            ABS(num) >= 1000000000, FORMAT(DIVIDE(num,1000000000),"0.00") & "B",
			            ABS(num) >= 1000000,    FORMAT(DIVIDE(num,1000000),"0.00") & "M",
			            ABS(num) >= 1000,       FORMAT(DIVIDE(num,1000),"0.00") & "K",
			            FORMAT(num,"0.00")
			        )
			
			/* ---------- Orders ---------- */
			VAR TodayOrders =
			    CALCULATE(MAX('Food Orders Count Last 7 Days Line'[COUNT_id]), 'Food Orders Count Last 7 Days Line'[time_period] = "Daily Trend")
			VAR SDLWOrders =
			    CALCULATE(MAX('Food Orders Count Last 7 Days Line'[COUNT_id]), 'Food Orders Count Last 7 Days Line'[time_period] = "1 Week Ago")
